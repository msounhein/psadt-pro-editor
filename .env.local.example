# Python Path for Local Embedding Service
# Point this to your Python executable in the virtual environment
PSADT_PYTHON_PATH=C:\path\to\psadt-pro-ui\.venv\Scripts\python.exe

# Where to store scripts and logs (defaults to AppData or temp directory)
PSADT_DATA_DIR=C:\path\to\data\directory

# Set to 'true' to enable debugging
PSADT_DEBUG=false

# Qdrant Database Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_api_key_if_needed
QDRANT_COLLECTION=psadt_commands
